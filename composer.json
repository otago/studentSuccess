{
    "name": "op/studentsuccess",
    "description": "Student Success website",
    "type": "silverstripe-project",
    "require": {
        "php": ">=5.6",
        "dnadesign/silverstripe-elemental": "^4.8.1",
        "dnadesign/silverstripe-elemental-list": "^1.2",
        "silverstripe/recipe-cms": "^4.10.0",
        "silverstripe/userforms": "^5.12.0",
        "silverstripe/widgets": "^2.2.0"
    },
    "config": {
        "process-timeout": 900,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true
        }
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    }
}
