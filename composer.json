{
    "name": "op/studentsuccess",
    "description": "Student Success website",
    "type": "silverstripe-project",
    "require": {
        "php": ">=8.0",
        "dnadesign/silverstripe-elemental": "^4.9",
        "dnadesign/silverstripe-elemental-list": "^1.2",
        "silverstripe/crontask": "^2.4",
        "silverstripe/display-logic": "^2.0",
        "silverstripe/dynamodb": "^4.0",
        "silverstripe/externallinks": "^2.2",
        "silverstripe/fulltextsearch": "^3.11",
        "silverstripe/recipe-cms": "^4.11.0",
        "silverstripe/saml": "2.0.0",
        "silverstripe/userforms": "^5.13",
        "silverstripe/widgets": "^2.3",
        "symbiote/silverstripe-gridfieldextensions": "^3.4",
        "symbiote/silverstripe-queuedjobs": "^4.10"
    },
    "config": {
        "process-timeout": 900,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true
        }
    },
    "minimum-stability": "RC",
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "require-dev": {
        "jdorn/sql-formatter": "1.3.x-dev"
    }
}
